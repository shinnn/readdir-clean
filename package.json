{
  "name": "readdir-clean",
  "version": "0.1.0",
  "description": "Like `fs.readdir`, but excludes contents automatically generated by OS, for example .DS_Store and Thumbs.db",
  "repository": "shinnn/readdir-clean",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --format=codeframe index.js test.js",
    "test": "nyc node test.js"
  },
  "license": "CC0-1.0",
  "files": [
    "index.js"
  ],
  "keywords": [
    "directory",
    "readdir",
    "file",
    "files",
    "contents",
    "fs",
    "list",
    "enumerate",
    "array",
    "clean",
    "filter",
    "promise",
    "async",
    "asynchronous"
  ],
  "dependencies": {
    "graceful-fs": "^4.1.11",
    "junk": "^1.0.3",
    "pinkie-promise": "^2.0.1"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^4.0.0",
    "eslint": "^4.0.0",
    "nyc": "^11.0.2",
    "rmfr": "^1.0.3",
    "tape": "^4.6.3",
    "write-file-atomically": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "@shinnn/node",
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off"
    }
  }
}
